<?xml version="1.0" encoding="utf-8"?>
<?xml-stylesheet type="text/xsl" href="objects.xsl"?>
<objects>
    <object type="tensor">
        <description lang="RU">Тензор, состоящий из 20 цветных изображений размера 200x500px (100000px)</description>  
        <dimensions>
            <dimension axis="X">20</dimension>
            <dimension axis="Y">3</dimension>
            <dimension axis="Z">100000</dimension>
        </dimensions>
    </object>

    <object type="matrix">
        <description lang="RU">Матрица, является ч/б изображением размером 200x500px</description>  
        <dimension_X>200</dimension_X>
        <dimension_Y>500</dimension_Y>
    </object>

    <object type="array">
        <description lang="RU">Массив, является чб изображением размера 32x32px (1024px)</description>  
        <length>1024</length>
    </object>

    <object type="array">
        <description lang="RU">Массив чисел 1-100</description>  
        <length>100</length>
    </object>

    <object type="matrix">
        <description lang="RU">Обучающая выборка, 15 признаков, 10000 обьектов</description>  
        <dimension_X>15</dimension_X>
        <dimension_Y>10000</dimension_Y>
    </object>

    <object type="matrix">
        <description lang="RU">Матрица, является ч/б изображением размером 64x64px</description>  
        <dimension_X>64</dimension_X>
        <dimension_Y>64</dimension_Y>
    </object>
</objects>